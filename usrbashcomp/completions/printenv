# printenv(1) completion                                   -*- shell-script -*-

_printenv()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        --help | --version)
            return
            ;;
    esac

    if [[ $cur == -* ]]; then
        COMPREPLY=($(compgen -W '$(_parse_help "$1")' -- "$cur"))
        return
    fi

    COMPREPLY=($(compgen -v -- "$cur"))
} &&
    complete -F _printenv printenv

# ex: filetype=sh
